dll)  DLL_USAGE="LIBRARIES:   build as dynamic by default" ;;
lib)
   if test "$with_dll" = "no" ; then
      DLL_USAGE="LIBRARIES:   build as static only"
   else
      DLL_USAGE="LIBRARIES:   build as static by default"
   fi
   ;;
esac

