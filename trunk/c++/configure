  GCC:3* )
    # Need to specify runpath for compiler-provided libraries
    libgcc_s=`$CXX -print-file-name=libgcc_s.so`
    case "$host_os" in
       osf*) LDFLAGS="-Wl,-rpath,`dirname $libgcc_s` $LDFLAGS" ;;
       *)    LDFLAGS="-Wl,-R`dirname $libgcc_s` $LDFLAGS" ;;
    esac
    ;;
esac

