for x_arg in "$@" ; do
   if test -z "$x_arg" ; then
      continue
   fi

   x_okay=
   for x_desc in $x_with_list ; do
      if test "$x_arg" = "$x_desc" ; then
         x_okay="okay"
         break
      fi
   done
   if test "$x_okay" = "okay" ; then
      continue
   fi

   case "$x_arg" in
      --with-extra-action= | --exec-prefix= \
      | --cache-file= | --build= | --host= | --target= | --with-runpath= )
      { { echo "$as_me:1374: error: \"  $x_arg:  requires value;  use --help to show usage\"" >&5
echo "$as_me: error: \"  $x_arg:  requires value;  use --help to show usage\"" >&2;}
   { (exit 1); exit 1; }; }
      ;;

      --with-extra-action=* | --exec-prefix=* | --with-build-root=* \
      | --with-projects=* | --with-check=* \
      | --cache-file=* | --build=* | --host=* \
      | [A-Z]*=* \
      | --with-sybase-local=* | --with-wxwin=* | --with-orbacus=* \
      | --target=* | --with-runpath=* | --no-create | --no-recursion)
      ;;

      * )
      { { echo "$as_me:1388: error: \"  $x_arg:  unknown option;  use --help to show usage\"" >&5
echo "$as_me: error: \"  $x_arg:  unknown option;  use --help to show usage\"" >&2;}
   { (exit 1); exit 1; }; }
      ;;
   esac
done

