#### Provide default environment setup for known platforms/compilers
compiler_extra=
case "$host:$compiler" in
 sparc-sun-solaris2\.6:GCC )
   echo "GCC compiler is not supported for:  $host"
   compiler=
   ;;

 *-linux*:GCC )
   NETWORK_LIBS=${NETWORK_LIBS:=" "}
   ;;

 *-osf*:GCC )
   CXXFLAGS="-fsquangle $CXXFLAGS"
   NETWORK_LIBS=${NETWORK_LIBS:=" "}
   ;;

 *:WorkShop5 )
   with_bincopy=${with_bincopy:="no"}
   if test -n "$WS64_CFLAGS" ; then
      if test "$host" != "sparc-sun-solaris2.7" ; then
        echo "64bit flags are for sparc-sun-solaris2.7 only:  $WS64_CFLAGS"
        exit 1
      fi
      compiler_extra="_64"
      with_internal="no"
   else
      compiler_extra="_32"
   fi
   CFLAGS="-mt $WS64_CFLAGS"
   CXXFLAGS="-mt +w +w2 $WS64_CFLAGS"
   LDFLAGS="-xildoff $WS64_CFLAGS $LDFLAGS"
   AR="$CXX -xar -o"
   RANLIB=":"
   ;;

 *:MIPSpro73 )
   COMMON_FLAGS="-fullwarn -signed -brief_diagnostics -use_readonly_const -G0 -rdata_shared -n32"
   CFLAGS="$COMMON_FLAGS -ansi"
   CXXFLAGS="$COMMON_FLAGS -ansiW -LANG:std -LANG:ansi-for-init-scope -woff 1460,1521,1429,1169,1209,1107,1424"
   CPPFLAGS="-D__LONGLONG $CPPFLAGS"
   LDFLAGS="-LANG:std $LDFLAGS"
   AR=${AR:="$CXX -ar -WR,-v -o"}
   ;;

 i*86*-pc-cygwin32:GCC )
   NETWORK_LIBS=${NETWORK_LIBS:=" "}
   CONF_exe_ext=".exe"
   ;;

 i*86*-pc-cygwin32:MSVC )
   CFLAGS="/W4 /Od /Zi /DEBUG /D_DEBUG"
   CXXFLAGS="/W4 /Od /Zi /DEBUG /D_DEBUG"
   CPPFLAGS="/nologo $CPPFLAGS"
   LDFLAGS=${LDFLAGS:="/DEBUG"}
   AR="lib.exe"
   RANLIB=":"
   STRIP=":"
   CONF_obj_ext=".obj"
   CONF_lib_ext=".lib"
   CONF_lib_l_ext=".lib"
   CONF_lib_pre=" "
   CONF_lib_l_pre=" "
   CONF_exe_ext=".exe"
   CONF_f_compile="/c "
   CONF_f_outobj="/Fo"
   CONF_f_outlib="/OUT:"
   CONF_f_libpath="/LIBPATH:"
   CONF_f_outexe="/OUT:"
   script_shell=""
   make_shell=""
   ;;
esac


