  shift
  CC="$x_CC $*"
  echo "adjusted C   compiler: $CC"
fi
x_CC=

set $CXX
x_CXX="`which $1`"
if test -x "$x_CXX" ; then
  shift
  CXX="$x_CXX $*"
  echo "adjusted C++ compiler: $CXX"
fi
