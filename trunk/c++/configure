#### Detect supported compilers
compiler=
compiler_ver=
if test "$GCC" = "yes" ; then
   compiler_ver="`$CXX --version 2>&1`"
   case "$compiler_ver" in
     2\.95* )  compiler="GCC" ;;
   esac
else
   case "$host" in
    sparc-sun-solaris2\.[67]  |  i*86-pc-solaris2\.[67] )
      compiler_ver="`$CXX -V 2>&1`"
      case "$compiler_ver" in
        *WorkShop*Compilers*5\.*)  compiler="WorkShop5" ;;
      esac ;;

    mips-sgi-irix6\.* )
      compiler_ver="`$CXX -version 2>&1`"
      case "$compiler_ver" in
        MIPSpro*Compilers:*Version*7\.3* )  compiler="MIPSpro73" ;;
      esac ;;

    i*86*-pc-cygwin32 )
      case "$CXX" in
        *cl\.exe* | cl)  compiler="MSVC" ;; 
      esac ;;
   esac
fi


