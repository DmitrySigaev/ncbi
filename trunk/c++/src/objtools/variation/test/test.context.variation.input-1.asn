Variation ::= {
  id {
    db "dbSNP",
    tag str "rs1"
  },
  placements {
    {
      loc pnt {
        point 2338229,
        id other {
          accession "NC_000001",
          version 10
        }
      },
      mol genomic
    }
  },
  variant-prop {
    version 5,
    resource-link preserved,
    gene-location 0,
    effect no-change,
    mapping 0,
    frequency-based-validation 0,
    genotype 0,
    quality-check 0
  },
  data set {
    type package,
    variations {
      {
        data instance {
          type ins,
          delta {
            {
              seq literal {
                length 1,
                seq-data iupacna "T"
              },
              action ins-before
            }
          }
        }
      }
    }
  },
  ext {
    {
      type str "VcfAttributes",
      data {
        {
          label str "filter",
          data str "PASS"
        },
        {
          label str "info",
          data str "DB"
        }
      }
    },
    {
      type str "Description",
      data {
        {
          label str "Input",
          data str "This is inserting a T, between a G and a C - so no change needed"
        }
      }
    }
  }
}
