# Build library "ncbi_xloader_sra"
###############################

LIB = ncbi_xloader_sra
SRC = sraloader

REQUIRES = objects

ASN_DEP = seqset seq

LIB_OR_DLL = both

DLL_LIB = sraread$(STATIC) $(SRAREAD_LDEP) $(SOBJMGR_LIBS)

CPPFLAGS = $(SRA_INCLUDE) -DNCBI_XLOADER_SRA_EXPORTS $(ORIG_CPPFLAGS)

WATCHERS = vasilche


USES_LIBRARIES =  \
    biblio general genome_collection medline ncbi-vdb-read pub seq \
    seqcode seqedit seqset sequtil sraread xncbi xobjmgr xser xutil
