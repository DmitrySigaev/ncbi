#! /bin/sh
#  $Id$

cd "@srcdir@"  ||  exit 2

if test ! -d webenv ;  then
    mkdir webenv
else
    rm webenv/*.[hc]pp
fi

if test ! -x "@build_root@/bin/datatool" ;  then
    echo Error: datatool not found
    exit 3
fi

"@build_root@/bin/datatool" \
    -m twebenv.asn -oA -oph webenv -opc webenv -oc webenv -orq -pch ncbi_pch.hpp

sed 's% "% "webenv/%' webenv/webenv__.cpp  > we_cpp__.cpp
sed 's% "% "webenv/%' webenv/webenv___.cpp > we_cpp___.cpp
rm webenv/webenv__*.[hc]pp
