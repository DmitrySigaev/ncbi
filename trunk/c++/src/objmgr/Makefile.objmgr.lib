      seq_map seq_map_ext seq_map_ci \
      align_ci annot_object annot_types_ci \
      data_loader desc_ci feat_ci graph_ci handle_range \
      handle_range_map object_manager reader reader_id1 reader_pubseq \
      seq_vector seq_vector_ci seqdesc_ci seq_id_handle seq_id_mapper \
      seq_id_tree gbload_util gbloader pack_string reader_zlib \
      seq_annot_ci

LIB    = xobjmgr

# Dependencies for shared library
DLL_LIB = id1 seqset $(SEQ_LIBS) pub medline biblio \
      general dbapi_driver xser xutil xcompress z bz2 xconnect xncbi
