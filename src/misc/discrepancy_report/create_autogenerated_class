#!/bin/bash

# Run this to update the autogenerated class.

# Don't worry right now about the details of what's going on here.

src_dir=/home/chenj/DisRepLib/trunk/c++/src/misc/discrepancy_report
inc_dir=/home/chenj/DisRepLib/trunk/c++/include/misc/discrepancy_report
path=$NCBI/c++.current/src/objects

datatool -pch ncbi_pch.hpp -m $path/seqset/seqset.asn  -m $path/general/general.asn -m $path/seq/seq.asn  -m $path/seqloc/seqloc.asn -m $path/seqfeat/seqfeat.asn -m $path/seqblock/seqblock.asn  -m $path/seqalign/seqalign.asn -m $path/pub/pub.asn  -m $path/biblio/biblio.asn  -m $path/seqres/seqres.asn -m $path/seqtable/seqtable.asn -m $path/medline/medline.asn -m $path/submit/submit.asn -oA -tvs auto_disc.txt

mv cauto_disc_class.cpp $src_dir
mv hauto_disc_class.hpp $inc_dir
rm -r *.[ch]pp
