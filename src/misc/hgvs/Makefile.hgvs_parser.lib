# $Id$


LIB = hgvs
SRC = hgvs_parser2 hgvs_writer2 variation_util2 \
      seq_id_resolver objcoords sequtils hgvs_reader

ASN_DEP = seq variation objcoords

ifneq "" "$(filter GCC_41% GCC_42%,$(signature))"
 ifneq "$(OSTYPE)" "darwin"
  hgvs_parser.o hgvs_parser2.o: CXXFLAGS = $(ORIG_CXXFLAGS:-O2=-O)
 endif
endif

DLL_LIB = objcoords xobjmgr

CPPFLAGS = $(ORIG_CPPFLAGS) $(BOOST_INCLUDE) -Wno-unused-local-typedefs
LIBS = $(CMPRS_LIBS) $(ORIG_LIBS)
REQUIRES = Boost.Spirit

WATCHERS = astashya

PROJ_TAG = gbench

USES_LIBRARIES =  \
    biblio connect connext creaders dbapi_driver entrez2 entrez2cli \
    general genome_collection id1 id2 medline ncbi_xloader_genbank \
    ncbi_xreader ncbi_xreader_cache ncbi_xreader_id1 ncbi_xreader_id2 \
    ncbi_xreader_pubseqos objcoords pub seq seqcode seqedit seqset \
    seqsplit sequtil submit variation xcompress xconnect xconnext \
    xncbi xobjmgr xobjread xobjutil xregexp xser xutil
