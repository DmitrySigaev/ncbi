[Compiler]

[Compiler.debug]
# /Ob1
InlineFunctionExpansion=1

[Compiler.release]
# Disable optimization (/Od)
Optimization=0
# Get rid of /Gy
EnableFunctionLevelLinking=FALSE
# Get rid of /Og
GlobalOptimizations=FALSE

[Linker]

[Linker.debug]
# Debug-mode Python on Windows wants a "_d" suffix
OutputFile = $(OutDir)\\_ncbi_d.dll

[Linker.release]
