# $Id$

# Meta-makefile("APP" project)
#################################

REQUIRES = objects algo

APP_PROJ = blastdbcmd makeblastdb blastdb_aliastool blastdbcheck convert2blastmask blastdbcp makeprofiledb 

srcdir = @srcdir@
include @builddir@/Makefile.meta

.PHONY: all $(APP_PROJ)

makeblastdb:
	make -f Makefile.makeblastdb_app

blastdbcmd:
	make -f Makefile.blastdbcmd_app

blastdbcp:
	make -f Makefile.blastdbcp_app

blastdb_aliastool:
	make -f Makefile.blastdb_aliastool_app

blastdbcheck:
	make -f Makefile.blastdbcheck_app

convert2blastmask:
	make -f Makefile.convert2blastmask_app

makeprofiledb:
	make -f Makefile.makeprofiledb_app

