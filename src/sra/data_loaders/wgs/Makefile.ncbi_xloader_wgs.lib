# $Id$

# Build library "ncbi_xloader_wgs"
###############################

PROJ_TAG = gbench,lib,public

LIB = ncbi_xloader_wgs
SRC = wgsloader wgsloader_impl

REQUIRES = objects

ASN_DEP = seqset seq

LIB_OR_DLL = both

DLL_LIB = id2cli id2 $(SOBJMGR_LIBS) sraread$(STATIC) $(SRAREAD_LDEP) $(CMPRS_LIB)
LIBS = $(SRA_SDK_SYSLIBS) $(ORIG_LIBS)
POST_LINK = $(VDB_POST_LINK)

CPPFLAGS = $(SRA_INCLUDE) -DNCBI_XLOADER_WGS_EXPORTS $(ORIG_CPPFLAGS)

WATCHERS = vasilche


USES_LIBRARIES =  \
    id2cli id2 $(SOBJMGR_LIBS) $(SRAREAD_LIBS) $(SRA_SDK_SYSLIBS)
