[seg]
PROGRAM=segify
HOST=ray.nlm.nih.gov
PORT=80
PATH=/cgi-bin/tools
FORMATIN=fasta
FLAGS=SEQ,DOC,PRT,TRG
SUBMENU=Secondary structure prediction
PROMPT_1=Window Size
PARAM_1=window
DESCRIPTION_1=window size for determining low-complexity segments
TYPE_1=text
DEFAULT_1=12
REQUIRED_1=FALSE
IMPORTANCE_1=
GROUP_1=Algorithm
HELP_1=window size for determining low-complexity segments
PROMPT_2=Trigger Complexity
PARAM_2=trigger
DESCRIPTION_2=trigger complexity for determining low-complexity segments
TYPE_2=text
DEFAULT_2=2.2
REQUIRED_2=FALSE
IMPORTANCE_2=
GROUP_2=Algorithm
HELP_2=trigger complexity for determining low-complexity segments
PROMPT_3=Extension Complexity
PARAM_3=extension
DESCRIPTION_3=extension complexity for determining low-complexity segments
TYPE_3=text
DEFAULT_3=
REQUIRED_3=FALSE
IMPORTANCE_3=
GROUP_3=Algorithm
HELP_3=extension complexity for determining low-complexity segments
PROMPT_4=Output Style
PARAM_4=output_style
DESCRIPTION_4=output style
TYPE_4=popup
CHOICES_4=fasta,tree,block
DEFAULT_4=fasta
REQUIRED_4=FALSE
IMPORTANCE_4=
GROUP_4=Interface
HELP_4=Format of the output. Default is 'tree'.
PROMPT_5=Fasta Segmenting Style
PARAM_5=segment
DESCRIPTION_5=how to show segments in fasta output
TYPE_5=popup
CHOICES_5=none,low,high,all
DEFAULT_5=
REQUIRED_5=FALSE
IMPORTANCE_5=
GROUP_5=Interface
HELP_5=In fasta output style, this option may be used to request only low-complexity segments, only high-complexity segments, or all segments to be returned in separate records. Default is 'none', which indicates that the sequence should not be segmented into separate records, but returned in a single record, with low-complexity segments represented by the 'x' character.
PROMPT_6=Minimum High-Complexity Segment Length
PARAM_6=min_length
DESCRIPTION_6=minimum length for high-complexity segments
TYPE_6=text
DEFAULT_6=0
REQUIRED_6=FALSE
IMPORTANCE_6=
GROUP_6=Algorithm
HELP_6=minimum length of low complexity regions. Default is zero.
PROMPT_7=Maximum Trimming
PARAM_7=max_trim
DESCRIPTION_7=maximum trimming of raw segment
TYPE_7=text
DEFAULT_7=100
REQUIRED_7=FALSE
IMPORTANCE_7=
GROUP_7=Algorithm
HELP_7=maximum trimming of raw segment. Default is 100.
PROMPT_8=Characters per Line
PARAM_8=characters
DESCRIPTION_8=characters per line in the output
TYPE_8=text
DEFAULT_8=60
REQUIRED_8=FALSE
IMPORTANCE_8=
GROUP_8=
HELP_8=number of sequence characters per line on the output. Set to zero for fit-to-screen.
PROMPT_9=Merge Low-Complexity Regions
PARAM_9=merge
DESCRIPTION_9=merge overlapping low-complexity segments
TYPE_9=checkbox
DEFAULT_9=TRUE
REQUIRED_9=FALSE
IMPORTANCE_9=
GROUP_9=Algorithm
HELP_9=merge adjacent low-complexity regions. Default is true.
PROMPT_10=Add Complexity Information to Defline
PARAM_10=defline_info
DESCRIPTION_10=add complexity information to defline
TYPE_10=checkbox
DEFAULT_10=TRUE
REQUIRED_10=FALSE
IMPORTANCE_10=
GROUP_10=Interface
HELP_10=When returning multiple segments of a sequence in fasta output style, this option controls whether complexity information is added to the definition line or each segment. Default is true.


